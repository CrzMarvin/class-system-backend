{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Audience",
    "description": "The Audience",
    "required": [
        "name",
        "location"
    ],
    "additionalProperties": false,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the audience."
        },

        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name of audience."
        },
        "location": {
            "$id": "#/properties/location",
            "type": "string",
            "title": "The location name of the audience."
        }
    }
}