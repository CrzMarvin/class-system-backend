{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Classroom",
    "description": "The Classroom",
    "required": [
        "name",
        "audience_id"
    ],
    "additionalProperties": true,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the classroom."
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name of classroom."
        },
        "icon_id": {
            "$id": "#/properties/icon_id",
            "type": "number",
            "title": "The id of the icon."
        },
        "audience_id": {
            "$id": "#/properties/audience_id",
            "type": "number",
            "title": "The id of the audiecne."
        }
    }
}