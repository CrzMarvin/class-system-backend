{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Teacher",
    "description": "The Teacher",
    "required": [
        "name"
    ],
    "additionalProperties": true,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the teacher."
        },
        "gender": {
            "$id": "#/properties/gender",
            "type": "string",
            "title": "The gender of teacher.",
            "enum": ["MALE", "FEMALE"]
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name of teacher."
        },
        "icon_id": {
            "$id": "#/properties/icon_id",
            "type": "number",
            "title": "The id of the icon."
        },
        "star": {
            "$id": "#/properties/star",
            "type": "integer",
            "title": "The star of teacher."
        }
    }
}