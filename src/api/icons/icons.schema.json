{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Icon",
    "description": "The Icon",
    "required": [
        "name",
        "type",
        "base_url",
        "resource"
    ],
    "additionalProperties": false,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the icon."
        },
        "type": {
            "$id": "#/properties/type",
            "type": "string",
            "title": "The type of icon.",
            "enum": ["AVATAR", "ICON"]
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The name of icon."
        },
        "base_url": {
            "$id": "#/properties/base_url",
            "type": "string",
            "title": "The base_url of the icon."
        },
        "resource": {
            "$id": "#/properties/resource",
            "type": "string",
            "title": "The resource name of the icon."
        }
    }
}